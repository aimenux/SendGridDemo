{
  "Logging": {
    "LogLevel": {
      "Default": "Trace",
      "Microsoft": "Information"
    },
    "Console": {
      "LogLevel": {
        "Default": "Trace",
        "Microsoft": "Warning"
      },
      "FormatterName": "simple",
      "FormatterOptions": {
        "SingleLine": true,
        "IncludeScopes": true,
        "UseUtcTimestamp": true,
        "TimestampFormat": "[HH:mm:ss:fff] "
      }
    }
  },
  "Settings": {
    "ApiKey": "!PUT-API-KEY!",
    "FromEmail": "!PUT-SENDER-EMAIL!",
    "ToEmail": "!PUT-RECEIVER-EMAIL!",
    "Subject": "!PUT-SUBJECT!",
    "TextEmailBody": "!PUT-TEXT-BODY!",
    "HtmlEmailBody": "!PUT-HTML-BODY!",
    "TemplateId": "!PUT-TEMPLATE-ID!",
    "AttachmentFilePath": "!PUT-ATTACHMENT-FILE-PATH!"
  } 
}